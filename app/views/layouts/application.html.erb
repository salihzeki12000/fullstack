<!DOCTYPE html>
<html>
  <head>
    <title>Peerspace</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.12.0/css/all.css" integrity="sha384-REHJTs1r2ErKBuJB0fCK99gCYsVjwxHrSU0N7I1zl9vZbggVJXRMsv/sLlOAGb4M" crossorigin="anonymous">
    
  </head>

  <body>
    <script type="text/javascript">
      window.logo = "<%= image_url('logo.png') %>"
      window.home = "<%= image_url('home.png') %>"
      window.meetings = "<%= image_url('meetings.png') %>"
      window.filmshoots = "<%= image_url('filmshoots.png') %>"
      window.birthday = "<%= image_url('birthday.png') %>"
      window.photoshoots = "<%= image_url('photoshoots.png') %>"
      window.workshops = "<%= image_url('workshops.png') %>"
      window.babyshower = "<%= image_url('babyshower.png') %>"
      window.dinners = "<%= image_url('dinners.png') %>"
      window.weddings = "<%= image_url('weddings.png') %>"
      window.teamofsides = "<%= image_url('teamofsides.png') %>"
      window.launchparties = "<%= image_url('launchparties.png') %>"
      window.outdoor = "<%= image_url('outdoor.png') %>"
      window.performances = "<%= image_url('performances.png') %>"
      window.specialevents = "<%= image_url('specialevents.png') %>"
      window.eventsbg = "<%= image_url('events-bg.png') %>"
      window.footerlogo = "<%= image_url('footerlogo.png') %>"
    </script>
    <%= yield %>
  </body>
</html>
